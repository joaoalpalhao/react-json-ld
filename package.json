{
  "name": "@joaoalpalhao/react-json-ld",
  "description": "A markup way to build your json ld scripts",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "author": "João Alpalhão",
  "repository": {
    "type": "git",
    "url": "https://github.com/joaoalpalhao/react-json-ld"
  },
  "keywords": [
    "react-json-ld",
    "react",
    "json-ld"
  ],
  "module": "dist/react-json-ld.esm.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/react-json-ld.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-json-ld.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@testing-library/react": "^13.1.1",
    "@types/react-dom": "^18.0.3",
    "husky": "^7.0.4",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "tiny-warning": "^1.0.3"
  }
}